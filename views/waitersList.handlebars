<div class="column">
  <div class="row">
  </div>
  <div class="w-100"></div>
  <div class="row justify-content-between">
    <div class="col-4"></div>
    <div class="col-6">
      <h3 class="text-center text-secondary welcome">Working days</h3>
      <div class="w-100"></div>
      <div class="row mt-3">
        <div class="col table-responsive">
          <table class="table">
            <tr>
              {{#each weekdays}}
              <th>
                {{day}}
              </th>
              {{/each}}
            </tr>
            <tr>
              {{#each weekdays}}
              <td>
                <ul>
                  {{#each waiters}}
                  <li>{{this}}</li>
                  {{/each}}
                </ul>
              </td>
              {{/each}}
            </tr>
          </table>
        </div>
      </div>
    </div>
  </div>
  <a href="/reset"><button type="button" class="btn">RESET</button></a>
</div>